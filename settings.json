{
  "editor.fontFamily": "Fira Code",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.snippetSuggestions": "top",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.tabCompletion": "on",
  "emmet.showSuggestionsAsSnippets": true,
  "editor.suggest.preview": true,
  "editor.lineHeight": 25,
  "editor.fontSize": 17,
  "editor.fontWeight": "bold",
  "editor.letterSpacing": 0.5,
  "editor.renderLineHighlight": "all",
  "files.trimTrailingWhitespace": true,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.insertFinalNewline": true,
  "editor.codeActionsOnSave": { "source.fixAll": true, "source.fixAll.eslint": true },
  "eslint.packageManager": "yarn",
  "npm.packageManager": "yarn",
  "eslint.run": "onSave",
  "eslint.runtime": "/usr/local/bin/node",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "eslint.format.enable": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.closeWhenEmpty": true,
  "editor.autoClosingQuotes": "beforeWhitespace",
  "telemetry.telemetryLevel": "off",
  "html.format.endWithNewline": true,
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "emmet.includeLanguages": { "javascript": "javascriptreact", "typescript": "javascriptreact" },
  "emmet.triggerExpansionOnTab": true,
  "svg.completion.emmet": true,
  "svg.completion.showAdvanced": true,
  "svg.completion.showDeprecated": true,
  "svg.minify.sortAttrs": true,
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "editor.scrollbar.verticalScrollbarSize": 5,
  "editor.scrollbar.horizontalScrollbarSize": 5,
  "search.searchOnType": false,
  "workbench.startupEditor": "none",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.tabCloseButton": "off",
  "vim.leader": ",",
  "vim.camelCaseMotion.enable": true,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.textwidth": 110,
  "vim.sneak": false,
	"vim.surround": true,
	"vim.useCtrlKeys": false,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "$HOME/.config/.vsvimrc",
  "vim.hlsearch": true,
	"vim.incsearch": true,
  "vim.normalModeKeyBindings": [
    {"before": ["<tab>"], "commands": ["workbench.action.nextEditor"]},
    {"before": ["<S-tab>"], "commands": ["workbench.action.previousEditor"]},
    {"before": [ "<space>" ], "commands": [ "whichkey.show" ]}
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {"before": ["H"], "after": ["^"]},
    {"before": ["L"], "after": ["$"]},
    {"before": ["<leader>", "f"], "commands": ["editor.action.formatDocument"]},
    // {"before": ["<leader>", "o"], "commands": ["editor.action.revealDefinition"]},
    {"before": ["<leader>", "O"], "commands": ["editor.action.showDefinitionPreviewHover"]},
    {"before": ["<leader>", "u"], "commands": ["editor.action.goToReferences"]},
		{"before": ["<leader>", "U"], "commands": ["references-view.findReferences"]},
    {"before": ["<leader>", "c", "h"], "commands": ["references-view.showCallHierarchy"]},
    {"before": ["<leader>", "c", "c"], "commands": ["merge-conflict.accept.current"]},
    {"before": ["<leader>", "c", "b"], "commands": ["merge-conflict.accept.both"]},
    {"before": ["<leader>", "c", "i"], "commands": ["merge-conflict.accept.incoming"]},
    {"before": ["<leader>", "c", "s"], "commands": ["merge-conflict.accept.selection"]},
    {"before": ["<leader>", "d", "r"], "commands": ["workbench.action.files.save", "workbench.action.debug.run"]},
		{"before": ["<leader>", "d", "s"], "commands": ["workbench.action.files.save", "workbench.action.debug.start"]},
    {"before": ["<leader>", "d", "b"], "commands": ["editor.debug.action.toggleBreakpoint"]},
		{"before": ["<leader>", "d", "c"], "commands": ["editor.debug.action.conditionalBreakpoint"]},
    {"before": ["<leader>", "e", "k"], "commands": ["editor.action.marker.prev"]},
		{"before": ["<leader>", "e", "j"], "commands": ["editor.action.marker.next"]},
    {"before": ["<leader>", "h", "k"], "commands": ["workbench.action.editor.previousChange", "editor.action.dirtydiff.previous"]},
		{"before": ["<leader>", "h", "j"], "commands": ["workbench.action.editor.nextChange", "editor.action.dirtydiff.next"]},
    {"before": ["<leader>", "h", "s"], "commands": ["git.stage"]},
		{"before": ["<leader>", "h", "u"], "commands": ["git.unstage"]},
		{"before": ["<leader>", "h", "r"], "commands": ["git.revertSelectedRanges"]},
    {"before": ["<leader>", "r", "n"], "commands": ["editor.action.rename"]},
    {"before": ["<leader>", "r", "e"], "commands": ["revealInExplorer"]},
  ],
  "vim.visualModeKeyBindings": [{"before": ["<space>"], "commands": ["whichkey.show"]}],
  "vim.visualModeKeyBindingsNonRecursive": [
    {"before": ["<leader>", "f"], "commands": ["editor.action.formatSelection"]},
	],
  "vim.insertModeKeyBindings": [
    {"before": ["j", "k"], "after": ["<Esc>"]}
  ],
  "whichkey.bindings": [{
    "key": "e", "name": "Show tree/explorer view", "type": "conditional",
    "bindings": [{
      "key": "",
      "name": "default",
      "type": "command",
      "command": "workbench.view.explorer"
    }, {
      "key": "when:sideBarVisible && explorerViewletVisible",
      "name": "Hide explorer",
      "type": "command",
      "command": "workbench.action.toggleSidebarVisibility"
    }],
  },{
    "key": "o",
    "name": "Outline",
    "type": "command",
    "command": "workbench.action.toggleAuxiliaryBar",
  }, {
    "key": "g",
    "name": "Source contrl",
    "type": "command",
    "command": "workbench.view.scm",
  }, {
    "command": "workbench.view.search",
    "key": "w",
    "name": "Search Word",
    "type": "command"
  }, {
    "command": "search.action.openEditor",
    "key": "f",
    "name": "New Search Editor",
    "type": "command"
  }, {
    "command": "workbench.action.gotoSymbol",
    "key": "s",
    "name": "Search document symbol",
    "type": "command"
  }, {
    "command": "workbench.action.showAllSymbols",
    "key": "S",
    "name": "Search project symbol",
    "type": "command"
  }, {
    "command": "workbench.action.splitEditor",
    "key": "h",
    "name": "split editor horizontally",
    "type": "command"
  }, {
    "command": "workbench.action.splitEditorDown",
    "key": "v",
    "name": "split editor vertically",
    "type": "command"
  }],
  "gopls": { "buildFlags": ["-tags=wireinject"], "experimentalWorkspaceModule": true },
  "files.autoSave": "onWindowChange",
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "workbench.fontAliasing": "antialiased",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.folders.color": "#42a5f5",
  "workbench.editor.wrapTabs": true,
  "workbench.colorTheme": "Iceberg (rainglow)",
  "git.path": "/usr/local/bin/git",
  "explorer.openEditors.visible": 0,
  "workbench.editor.limit.value": 6,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.preferredLightColorTheme": "Fodder Light (rainglow)",
  "material-icon-theme.activeIconPack": "react",
  "window.autoDetectHighContrast": false,
  "git.decorations.enabled": false,
  "workbench.tree.indent": 14,
  "editor.cursorBlinking": "solid",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.selectionHighlight": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "files.associations": {
    "*.json": "jsonc"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "on",
    "strings": "off",
  },
  "extensions.autoUpdate": false,
  "editor.tokenColorCustomizations": {
    "[Stealth (rainglow)]": {
      "textMateRules": [{
        "scope": ["comment"],
        "settings": {
          "foreground": "#6d6d6d50"
        }
      }]
    }
  },
  "workbench.colorCustomizations": {
    "[Stealth (rainglow)]": {
      "list.activeSelectionBackground": "#135564",
      "editor.selectionBackground": "#888",
      "editor.selectionHighlightBackground": "#ffffff00",
      "editor.selectionHighlightBorder": "#ffffff44",
      "editor.findMatchBackground": "#f1eeb3a9",
      "editor.findMatchHighlightBackground": "#f1eeb3a9",
      "editor.findMatchBorder": "#ffff00cc",
      "editor.findMatchHighlightBorder": "#ff000080",
      "editor.lineHighlightBackground": "#ffffff00",
      "editor.lineHighlightBorder": "#ffffff00",
      "statusBar.debuggingBackground": "#410d18",
      "statusBar.debuggingForeground": "#dddddd",
      "editorBracketMatch.background": "#b58900",
      "editorBracketMatch.border": "#ffffff00",
      "editor.wordHighlightBorder": "#ffffff44",
      "editor.wordHighlightStrongBackground": "#ffffff00",
      "editorCursor.foreground": "#ff08",
      "editor.symbolHighlightBackground": "#ffffff00",
      "editorSuggestWidget.focusHighlightForeground": "#00afaf",
      "editorSuggestWidget.highlightForeground": "#00afaf",
      "editorBracketHighlight.foreground1": "#555",
      "editorBracketHighlight.foreground2": "#555",
      "editorBracketHighlight.foreground3": "#555",
      "editorBracketHighlight.foreground4": "#555",
      "editorBracketHighlight.foreground5": "#555",
      "editorBracketHighlight.foreground6": "#555",
      "editorBracketHighlight.unexpectedBracket.foreground": "#555",
      "editorIndentGuide.background": "#ffffff08",
      "editorIndentGuide.activeBackground": "#ffffff00",
      "diffEditor.insertedTextBackground": "#9bb95508",
      "diffEditor.removedTextBackground": "#ff000008",
    },
  },
  "editor.cursorSurroundingLines": 23,
}
